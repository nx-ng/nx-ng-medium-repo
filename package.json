{
  "name": "nx-ng-medium",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && nx print-affected --base=\"previous-ng-major\" --head=HEAD",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "benchmark:build:cold": "hyperfine --prepare 'rm -rf node_modules/.cache .angular/' 'NX_DAEMON=false yarn nx build app0' --export-json results/build-previous-ng-major-version-cold.json",
    "benchmark:build:cache": "hyperfine --warmup 1 'NX_DAEMON=false yarn nx build app0' --export-json results/build-previous-ng-major-version-cache.json",
    "benchmark:test:cold": "hyperfine 'NX_DAEMON=false yarn nx run-many --target=test --all --skip-nx-cache' --prepare 'yarn jest --clearCache' --max-runs 5 --export-json results/test-previous-ng-major-version-cold.json",
    "benchmark:lint:cold": "hyperfine --prepare 'rm -rf node_modules/.cache .angular/' 'NX_DAEMON=false yarn nx run-many --target=lint --all' --export-json results/lint-previous-ng-major-version-cold.json",
    "benchmark:e2e:cold": "hyperfine --prepare 'rm -rf node_modules/.cache .angular/' 'NX_DAEMON=false yarn nx e2e app0' --export-json results/e2e-previous-ng-major-version-cold.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@nrwl/angular": "13.0.0",
    "firebase": "^9.6.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@nrwl/cli": "13.0.0",
    "@nrwl/cypress": "13.0.0",
    "@nrwl/eslint-plugin-nx": "13.0.0",
    "@nrwl/jest": "13.0.0",
    "@nrwl/linter": "13.0.0",
    "@nrwl/tao": "13.0.0",
    "@nrwl/workspace": "13.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "cypress": "^8.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  }
}
