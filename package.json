{
  "name": "nx-ng-medium",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && nx print-affected --base=\"previous-nx-minor\" --head=HEAD",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "benchmark:build:cold": "hyperfine --prepare 'rm -rf node_modules/.cache .angular/' 'NX_DAEMON=false yarn nx build app0 --skip-nx-cache' --export-json results/build-previous-nx-minor-version-cold.json",
    "benchmark:build:cache": "hyperfine --warmup 1 'NX_DAEMON=false yarn nx build app0' --export-json results/build-previous-nx-minor-version-cache.json",
    "benchmark:test:cold": "hyperfine 'NX_DAEMON=false yarn nx run-many --target=test --all --skip-nx-cache' --prepare 'yarn jest --clearCache' --max-runs 5 --export-json results/test-previous-nx-minor-version-cold.json",
    "benchmark:lint:cold": "hyperfine --prepare 'rm -rf node_modules/.cache .angular/' 'NX_DAEMON=false yarn nx run-many --target=lint --all' --export-json results/lint-previous-nx-minor-version-cold.json",
    "benchmark:e2e:cold": "hyperfine --prepare 'rm -rf node_modules/.cache .angular/' 'NX_DAEMON=false yarn nx e2e app0' --export-json results/e2e-previous-nx-minor-version-cold.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@nrwl/angular": "14.1.9",
    "firebase": "^9.6.6",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.7",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "13.3.7",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@nrwl/cypress": "14.1.9",
    "@nrwl/eslint-plugin-nx": "14.1.9",
    "@nrwl/jest": "14.1.9",
    "@nrwl/linter": "14.1.9",
    "@nrwl/workspace": "14.1.9",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "cypress": "^9.1.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "11.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4",
    "nx": "14.1.9"
  }
}

